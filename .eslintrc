{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style": 0,
        "quotes": [2, "single", { "avoidEscape": true }],
        "array-bracket-spacing": ["error", "never"],
    }
   
}
